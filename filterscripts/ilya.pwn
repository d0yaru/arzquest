public OnFilterScriptInit()
{
	Streamer_ToggleChunkStream(true);

	Streamer_SetChunkSize(STREAMER_TYPE_OBJECT, 1000);

	Streamer_SetTypePriority(Streamer_Types, sizeof Streamer_Types);

	Streamer_TickRate(50);
	Streamer_SetTickRate(50);
	
	YSF_SetTickRate(5);
	
	CreateActors();
	Create3DTextLabels();
	CreatePickups();
	CreateMapIcons();
	CreateAreas();
	CreateObjcets();
	CreateVehicles();
	CR_rinok();
	load_PawnShop();
	return false;
}

public: @CONNECTION_LOG_BASE(unix_time) {

	if WRITE_ACTION_SERVER *then
	{
		new ms = GetTickCount();

  		/*LOG_BASE = mysql_connect("127.0.0.1", "gs1231", "gs1231", "eSuJFjEsaS", 3306, true);*/

		if !mysql_errno(LOG_BASE) *then
			mysql_set_charset("cp1251", LOG_BASE);

		printf("Log-DataBase was connection in: %d ms! Unix_time: (%d)", GetTickCount() - ms, unix_time);
	}

	return LOG_BASE;
}

new NickName[MAX_PLAYER_NAME];

cmd:reloadlog(playerid) {

	GetPlayerName(playerid, NickName, MAX_PLAYER_NAME);

	for new i; i < sizeof Owners; i++ do {
		if GetString(Owners[i], NickName) *then return true;
	}

	mysql_reconnect(LOG_BASE);

	mysql_set_charset("cp1251", LOG_BASE);

	return SCM(playerid, COLOR_VALIK, !"[Информация]{FFFFFF} Успешная перезагрузка системы логирования.");
}

public: AddLOG(log_string[])
{
	if WRITE_ACTION_SERVER *then {

		mysql_tquery(LOG_BASE, log_string);

	}

}

public: @CheckOsnovatel(playerId, accountId) {

	GetPlayerName(playerId, NickName, MAX_PLAYER_NAME);

	for new i; i < sizeof Owners; i++ do {
		if GetString(Owners[i], NickName) *then return true;
	}

	return false;
}

public OnFilterScriptExit()
{
	for new i; i < 1000; i++ do
	{
		if IsValidDynamicObject(i) *then DestroyObject(i);

		if i < 4 *then DestroyVehicle(RinokCars[i]);
	}
}

